        "ACH": {
            "caption": "transaction succeeds on checkout",
            "name": "onsuccess"
        },
        "ACI": {
            "caption": "customer closes checkout",
            "name": "onclose"
        },
        "ACJ": {
            "caption": "error occurs in checkout",
            "name": "onerror"
        },
        "ACM": {
            "caption": "customer loads checkout",
            "name": "onload"
        }
    },
    "fields": {
        "AAK": {
            "caption": "Pay button text",
            "default_val": "Pay with Paystack",
            "editor": "StaticText",
            "in_style": true,
            "name": "button_text",
            "rank": 1
        },
        "ACD": {
            "caption": "The email address of the customer",
            "default_val": "customer@email.com",
            "doc": "The email address of the customer",
            "editor": "DynamicValue",
            "name": "email",
            "rank": 3,
            "value": "text"
        },
        "ACE": {
            "caption": "transaction amount",
            "default_val": 10000,
            "doc": "The amount of the transaction in the smallest currency unit, i.e kobo for Naira transactions, pesewas for Cedis transactions etc.",
            "editor": "DynamicValue",
            "name": "amount",
            "rank": 4,
            "value": "number"
        },
        "ACF": {
            "caption": "plan code",
            "doc": "A valid Paystack plan code e.g. PLN_cujsmvoyq2209ws.",
            "editor": "DynamicValue",
            "name": "plan_code",
            "optional": true,
            "rank": 5,
            "value": "text"
        },
        "ACN": {
            "caption": "reference",
            "doc": "Unique case sensitive transaction reference. Only -,`.`, = and alphanumeric characters allowed. If not specified, a reference will be auto-generated for the transaction",
            "editor": "DynamicValue",
            "name": "reference",
            "optional": true,
            "rank": 6,
            "value": "text"
        },
        "ACU": {
            "caption": "show_badge",
